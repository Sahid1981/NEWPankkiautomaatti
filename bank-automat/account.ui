border: 1px solid rgba(0, 0, 0, 60);
border-radius: 12px;

font-size: 16px;
font-weight: 800;
padding: 12px 14px;
}

#btnLogOutVahvista:hover, #btnNostaVahvistaVahvista:hover, #btnNosta20:hover, #btnNosta40:hover, #btnNosta50:hover, #btnNosta100:hover, #btnNostaMuu:hover, #btnTapahtumatVasen:hover, #btnTapahtumatOikea:hover {
