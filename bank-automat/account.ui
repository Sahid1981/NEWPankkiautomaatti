}

/* Perus napit */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #2A5CFF,
                                stop:1 #1B3FBF);
    color: #FFFFFF;

    border: 1px solid rgba(0, 0, 0, 60);
    border-radius: 12px;

    font-size: 16px;
    font-weight: 800;
    padding: 12px 14px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #3A6DFF,
                                stop:1 #2449D8);
}

QPushButton:pressed {
   background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #1E47D6,
                                stop:1 #16339E);
    border-bottom: 1px solid rgba(0, 0, 0, 110);
    padding-top: 13px;  /* pieni “painallus” */
